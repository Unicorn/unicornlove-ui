{
  "name": "@unicornlove/ui",
  "version": "1.0.0",
  "description": "Comprehensive UI component library for Tamagui and Expo",
  "keywords": [
    "tamagui",
    "expo",
    "react-native",
    "ui",
    "components"
  ],
  "author": "Scaffald",
  "homepage": "https://github.com/Unicorn/unicornlove-ui",
  "bugs": {
    "url": "https://github.com/Unicorn/unicornlove-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Unicorn/unicornlove-ui"
  },
  "license": "MIT",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styleguide": {
      "types": "./dist/styleguide/index.d.ts",
      "import": "./dist/styleguide/index.mjs",
      "require": "./dist/styleguide/index.js"
    },
    "./types/geographic": {
      "types": "./dist/types/geographic.d.ts",
      "import": "./dist/types/geographic.mjs",
      "require": "./dist/types/geographic.js"
    },
    "./types/phone": {
      "types": "./dist/types/phone.d.ts",
      "import": "./dist/types/phone.mjs",
      "require": "./dist/types/phone.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "pnpm build",
    "check": "tamagui check",
    "check:type": "tsc --noEmit",
    "lint": "biome lint ./src",
    "lint:fix": "biome lint --write ./src",
    "format": "biome format ./src",
    "format:fix": "biome format --write ./src",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "release": "semantic-release",
    "release:dry-run": "semantic-release --dry-run"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.74.0",
    "expo": ">=51.0.0",
    "tamagui": ">=1.138.0",
    "@tamagui/core": ">=1.138.0",
    "react-hook-form": ">=7.0.0",
    "zod": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "expo": {
      "optional": true
    },
    "react-native": {
      "optional": true
    }
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "~3.1.0",
    "@tamagui/animations-moti": "^1.138.0",
    "@tamagui/animations-react-native": "^1.138.0",
    "@tamagui/font-inter": "^1.138.0",
    "@tamagui/helpers-icon": "^1.138.0",
    "@tamagui/lucide-icons": "^1.138.0",
    "@tamagui/theme-builder": "^1.138.0",
    "@tamagui/toast": "^1.138.0",
    "@tanstack/react-table": "~8.20.5",
    "@tiptap/core": "^2.27.1",
    "@tiptap/extension-placeholder": "^2.27.1",
    "@tiptap/extension-underline": "^2.27.1",
    "@tiptap/react": "^2.27.1",
    "@tiptap/starter-kit": "^2.27.1",
    "@ts-react/form": "~1.6.4",
    "expo-router": "~6.0.14",
    "moti": "~0.25.3",
    "react-dom": "19.1.0",
    "react-dropzone": "~14.3.8",
    "react-native-reanimated": "~4.1.2",
    "awesome-phonenumber": "~7.5.0"
  },
  "devDependencies": {
    "@biomejs/biome": "~2.3.6",
    "@testing-library/jest-dom": "^6.4.2",
    "@types/node": "~20.0.0",
    "@types/react": "~19.1.0",
    "tsup": "^8.0.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.10"
  }
}
